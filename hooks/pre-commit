#!/usr/bin/python3

import sys
import subprocess

print("running go test git hook")
process_output = subprocess.run(["go", "test", "github.com/ornlu-is/go_git_hooks_example"], capture_output=True)
print(process_output)
print(f"exiting with code {process_output.returncode}")

sys.exit(process_output.returncode) 

